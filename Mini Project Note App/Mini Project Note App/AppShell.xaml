<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Mini_Project_Note_App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Mini_Project_Note_App"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="HomePage"
        ContentTemplate="{DataTemplate local:HomePage}"
        Route="HomePage" />

    <ShellContent
        Title="EditNotePage"
        ContentTemplate="{DataTemplate local:EditNotePage}"
        Route="EditNotePage" />
    
    <ShellContent
        Title="NewNotePage"
        ContentTemplate="{DataTemplate local:NewNotePage}"
        Route="NewNotePage" />
    
</Shell>
